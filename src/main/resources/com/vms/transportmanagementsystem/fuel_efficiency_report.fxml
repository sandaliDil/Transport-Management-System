<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="444.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vms.transportmanagementsystem.controller.FuelEfficiencyController">

    <HBox spacing="10" />

    <TableView fx:id="efficiencyTable" layoutX="49.0" layoutY="75.0" prefHeight="339.0" prefWidth="622.0">
        <columns>
            <TableColumn fx:id="vehicleCol" prefWidth="103.0" text="Vehicle ID" />
            <TableColumn fx:id="distanceCol" prefWidth="173.0" text="Total Distance (km)" />
            <TableColumn fx:id="fuelQtyCol" prefWidth="158.0" text="Fuel Quantity (liters)" />
            <TableColumn fx:id="efficiencyCol" prefWidth="187.0" text="Efficiency (km/l)" />
        </columns>
    </TableView>
    <Button layoutX="480.0" layoutY="14.0" onAction="#onGenerateReport" prefHeight="33.0" prefWidth="114.0" text="Generate Report" />
     <ComboBox fx:id="yearComboBox" layoutX="294.0" layoutY="14.0" prefHeight="33.0" prefWidth="157.0" />
     <Label layoutX="246.0" layoutY="14.0" prefHeight="33.0" prefWidth="48.0" text="   Year:" textAlignment="CENTER" />
     <ComboBox fx:id="monthComboBox" layoutX="72.0" layoutY="14.0" prefHeight="33.0" prefWidth="150" />
     <Label layoutX="14.0" layoutY="18.0" prefHeight="25.0" prefWidth="48.0" text="Month:" />

</AnchorPane>
